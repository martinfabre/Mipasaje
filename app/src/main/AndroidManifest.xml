<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gsc.com.mipasaje" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/CustomActionBarTheme" >
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ReserveActivity"
            android:label="@string/title_activity_reserve"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".BusLayoutActivity"
            android:label="@string/title_activity_bus_layout"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".QRReaderActivity"
            android:label="@string/title_activity_qrreader"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".SignUpActivity"
            android:label="@string/title_activity_sign_up"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>
